{
  "name": "@capdiem/antd-components",
  "version": "0.8.0",
  "description": "Custom components which based on Antd Design",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepublishOnly": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -w -p .",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint --fix src --ext .ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/capdiem/antd-components.git"
  },
  "keywords": [
    "antd",
    "component"
  ],
  "author": "capdiem",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/capdiem/antd-components/issues"
  },
  "homepage": "https://github.com/capdiem/antd-components#readme",
  "dependencies": {
    "antd": "^3.25.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "radium": "^0.26.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.8",
    "@types/radium": "^0.24.2",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
